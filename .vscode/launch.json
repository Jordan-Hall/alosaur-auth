	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Deno",
			"request": "launch",
			"type": "pwa-node",
			"program": "auth/auth-server.ts",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "deno",
			"runtimeArgs": [
				"run",
				"--unstable",
				"--inspect-brk",
				"--allow-all",
				"--config=./tsconfig.json"
			],
			"attachSimplePort": 9229
		}
	]
}